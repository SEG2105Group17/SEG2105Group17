<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InstructorPersonalCoursesActivity">

    <TextView
        android:id="@+id/txtIID"
        android:layout_width="250dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="13dp"
        android:text="Select for ID"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtI1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtI1"
        android:layout_width="115dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Class ID"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editIName"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="13dp"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/txtI2"
        app:layout_constraintTop_toBottomOf="@+id/txtIID"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editIDate"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="13dp"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtI3"
        app:layout_constraintTop_toBottomOf="@+id/editIName"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editITime"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="13dp"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtI4"
        app:layout_constraintTop_toBottomOf="@+id/editIDate"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editIDifficulty"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="13dp"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtI5"
        app:layout_constraintTop_toBottomOf="@+id/editITime"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/editICapacity"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="13dp"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/txtI6"
        app:layout_constraintTop_toBottomOf="@+id/editIDifficulty"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/txtI3"
        android:layout_width="115dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="Date"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtI2" />

    <TextView
        android:id="@+id/txtI5"
        android:layout_width="115dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="Difficulty"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtI4" />

    <TextView
        android:id="@+id/txtI4"
        android:layout_width="115dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="Time"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtI3" />

    <TextView
        android:id="@+id/txtI2"
        android:layout_width="115dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:text="Class Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtI1" />

    <TextView
        android:id="@+id/txtI6"
        android:layout_width="115dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="Capacity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtI5" />

    <Button
        android:id="@+id/btnIAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="131dp"
        android:layout_marginTop="8dp"
        android:text="Add"
        android:onClick="newProduct"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editICapacity" />

    <Button
        android:id="@+id/btnIBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:onClick="backI"
        android:text="Back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editICapacity" />

    <Button
        android:id="@+id/btnIDelete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="13dp"
        android:onClick="removeProduct"
        android:text="Delete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editICapacity" />

    <Button
        android:id="@+id/btnIEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Edit"
        android:onClick="editProduct"
        app:layout_constraintEnd_toStartOf="@+id/btnIDelete"
        app:layout_constraintStart_toEndOf="@+id/btnIAdd"
        app:layout_constraintTop_toBottomOf="@+id/editICapacity" />

    <ListView
        android:id="@+id/listI"
        android:layout_width="410dp"
        android:layout_height="145dp"
        android:layout_marginTop="16dp"
        android:background="#E3F3EF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnIAdd" />

    <TextView
        android:id="@+id/txtClassInfo"
        android:layout_width="413dp"
        android:layout_height="137dp"
        android:background="#C7DEE1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listI" />
</androidx.constraintlayout.widget.ConstraintLayout>